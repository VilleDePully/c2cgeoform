{{form|safe}}
<script>
$(function () {
  $('#deform').data('serialize', $('#deform').serialize()); // On load save form current state
  $(window).bind('beforeunload', function(e){
    if($('#deform').serialize() != $('#deform').data('serialize')){
      return true;
    } else {
      e = null;
    }
  });
  $('button[type=submit]').click(function (e) {
    $(window).off('beforeunload')
  });
});
</script>
